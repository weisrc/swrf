{
  "name": "@swrf/core",
  "version": "0.0.1",
  "types": "./dist/types/browser/index.d.ts",
  "exports": {
    "browser": {
      "import": "./dist/browser.es.js",
      "require": "./dist/browser.cjs.js",
      "default": "./dist/browser.es.js"
    },
    "development": {
      "import": "./dist/browser.es.js",
      "require": "./dist/browser.cjs.js",
      "default": "./dist/browser.es.js"
    },
    "node": {
      "import": "./dist/node.es.js",
      "require": "./dist/node.cjs.js",
      "default": "./dist/node.es.js"
    }
  },
  "scripts": {
    "build:types": "tsc",
    "build:browser": "vite build --mode browser",
    "build:browser-dev": "vite build --mode browser-dev",
    "build:browser-slim": "vite build --mode browser-slim",
    "build": "run-p build:*"
  },
  "devDependencies": {
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
